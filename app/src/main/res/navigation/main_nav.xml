<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/classifierFragment">

    <fragment
        android:id="@+id/classifierFragment"
        android:name="com.example.imageclassificationdemo.ui.ClassifierFragment"
        android:label="fragment_classifier"
        tools:layout="@layout/fragment_classifier" >
        <action
            android:id="@+id/action_detail_navigation"
            app:destination="@id/detailClassifierFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailClassifierFragment"
        android:name="com.example.imageclassificationdemo.ui.DetailClassifierFragment"
        android:label="fragment_detail_classifier"
        tools:layout="@layout/fragment_detail_classifier" />
    <fragment
        android:id="@+id/resultatPlantationFragment"
        android:name="com.example.imageclassificationdemo.ui.ResultatPlantationFragment"
        android:label="fragment_resultat_plantation"
        tools:layout="@layout/fragment_resultat_plantation" />
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.example.imageclassificationdemo.ui.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />
</navigation>